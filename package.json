{
  "name": "collaborative-code-editor",
  "version": "1.0.0",
  "description": "Real-time collaborative code editor with multi-language execution support",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "nodemon server/index.js",
    "client:dev": "cd client && npm run dev",
    "install:all": "npm install && cd client && npm install",
    "build": "cd client && npm run build",
    "start": "node server/index.js"
  },
  "keywords": [
    "collaborative",
    "code-editor",
    "real-time",
    "websocket",
    "monaco-editor"
  ],
  "author": "Zaid Khan",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "socket.io": "^4.6.1",
    "redis": "^4.6.5",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "uuid": "^9.0.0",
    "dockerode": "^3.3.5",
    "tar-stream": "^3.1.6",
    "mongoose": "^7.0.3",
    "express-rate-limit": "^6.7.0",
    "helmet": "^7.0.0",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.22",
    "concurrently": "^8.0.1"
  }
}

